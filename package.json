{
  "name": "poa-bus",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll 1000 --aot -o --host 0.0.0.0",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "jest --coverage=false",
    "test:snapshot": "jest  --coverage=false --updateSnapshot",
    "test:coverage": "jest",
    "open-coverage": "google-chrome-stable coverage/lcov-report/index.html",
    "clean": "rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force",
    "reinstall": "npm run clean && npm install",
    "review": "ng lint && npm run test:coverage",
    "deploy": "npm run review && npm run build:prod && surge $(echo $PWD'/dist/poa-bus') poa-bus.surge.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/localize": "~9.1.11",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@ng-bootstrap/ng-bootstrap": "^6.2.0",
    "@types/jest": "^26.0.8",
    "bootstrap": "^4.4.0",
    "jest": "^26.2.2",
    "jest-preset-angular": "^8.2.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular/cli": "~9.1.8",
    "@angular/compiler-cli": "~9.1.11",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
